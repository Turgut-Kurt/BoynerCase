{
  "name": "BoynerCase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "clear && react-native run-android",
    "ios": "clear && react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "pod": "cd ios && pod install",
    "clear1": "cd android && ./gradlew clean && clear",
    "clear2": "cd ios && xcodebuild clean && clear",
    "reset": "react-native start --reset-cache",
    "variant": "npx react-native run-android --variant=release",
    "assets": "npx react-native-asset",
    "debug-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug && cd ..",
    "release-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && cd android && ./gradlew assembleRelease && cd ..",
    "build:ios": "react-native bundle --platform='ios' --dev=false --entry-file='index.js' --bundle-output='./ios/main.jsbundle' "
  },
  "dependencies": {
    "axios": "^0.27.2",
    "react": "18.1.0",
    "react-native": "0.70.4",
    "react-native-svg": "^13.4.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "prop-types": "^15.8.1",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
